<H1><%= @list.name  %></H1>

<p>Movies</p>
<% @list.bookmarks.each do |bookmark| %>
<ul>
  <li>
  <%= bookmark.movie.title %> <%= "- #{bookmark.comment}" %>
  </li>
</ul>
<% end %>

<%= link_to "Add a list", new_list_path %>
<%= link_to "Add a Bookmark", new_list_bookmark_path(@list) %>
